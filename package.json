{
  "name": "@scrolldomains/snsjs",
  "version": "3.0.0-alpha.37",
  "description": "SNS javascript library for contract interaction",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.mjs"
    },
    "./*.js": {
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.js",
      "default": "./dist/esm/*.mjs"
    },
    "./*.mjs": {
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.js",
      "default": "./dist/esm/*.mjs"
    },
    "./*": {
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.js",
      "default": "./dist/esm/*.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "contracts/*": [
        "./dist/types/contracts/*"
      ],
      "functions/*": [
        "./dist/types/functions/*"
      ],
      "utils/*": [
        "./dist/types/utils/*"
      ],
      "generated/*": [
        "./dist/types/generated/*"
      ]
    }
  },
  "files": [
    "dist/",
    "!src/tests"
  ],
  "repository": "git@github.com:snsdomains/snsjs-v3.git",
  "author": "luofeng.dev16091994@gmail.com",
  "license": "MIT",
  "scripts": {
    "tenv": "ens-test-env -a",
    "denv": "pnpm tenv start -ns -nb --extra-time 11368000",
    "generate-types": "ts-node scripts/runTypechain.ts",
    "generate-abis": "hardhat export-abi",
    "start": "ts-node --files src/index.test.ts",
    "test": "jest --run-in-band",
    "test:watch": "jest --watch --run-in-band",
    "test:specific": "ts-node --files ",
    "pub": "yarn publish --access public",
    "ver": "pnpm version --no-workspaces-update"
  },
  "dependencies": {
    "@ensdomains/address-encoder": "^0.2.18",
    "@ensdomains/content-hash": "^2.5.7",
    "@ensdomains/dnsprovejs": "^0.4.1",
    "@ensdomains/dnssecoraclejs": "^0.2.7",
    "dns-packet": "^5.3.1",
    "ethers": "^5.6.1",
    "graphql": "^16.3.0",
    "graphql-request": "next",
    "idna-uts46-hx": "3.4.0",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "@ensdomains/buffer": "^0.0.13",
    "@ensdomains/ens-contracts": "^0.0.15",
    "@ensdomains/ens-test-env": "latest",
    "@ethersproject/abi": "^5.6.0",
    "@ethersproject/providers": "^5.6.2",
    "@swc/core": "^1.2.162",
    "@swc/jest": "^0.2.20",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.4.1",
    "@types/traverse": "^0.6.32",
    "dotenv": "^16.0.0",
    "esbuild": "^0.15.6",
    "eslint-plugin-jest": "^27.0.1",
    "ganache": "^7.4.0",
    "glob": "^8.0.3",
    "jest": "^27.5.1",
    "jest-environment-node": "^27.5.1",
    "jest-localstorage-mock": "^2.4.21",
    "nock": "^13.2.8",
    "node-fetch": "^3.2.6",
    "node-fetch-commonjs": "^3.1.1",
    "solc": "^0.8.13",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1"
  },
  "peerDependencies": {
    "ethers": "*"
  }
}
